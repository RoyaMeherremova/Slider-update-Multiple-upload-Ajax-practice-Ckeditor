@model List<Product>

@foreach(var product in Model){

    <li class="d-flex px-3">
       
        <a asp-controller="Shop" asp-action="Detail" asp-route-id="@product.Id">@product.Name</a>  @*inputdan gelir actiona o Adda productlar tapilib gonderilir Bu partiala ve o productun Id-sine gore biz o productu basqa actiona gonderib yazdira bilirik yenide partiala*@
      
    </li>

}



